<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>FlameGraph.Net</PackageId>
    <Title>Create flame graph with .NET code easily.</Title>
    <Authors>xiaomi7732</Authors>
    <Company>xiaomi7732</Company>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/xiaomi7732/FlameGraphNet</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Svg" Version="3.1.1" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="template.svg" />
    <EmbeddedResource Include="script.min.js" />
  </ItemGroup>
</Project>
